    "def maxcontsum(arr,n):\n",
    "    cursum=0\n",
    "    maxsum=0\n",
    "    for i in range(n):\n",
    "        cursum=cursum+arr[i]\n",
    "        if(cursum>maxsum):\n",
    "            maxsum=cursum\n",
    "        if(cursum<0):\n",
    "            cursum=0\n",
    "        \n",
    "    return maxsum\n",
    "arr=[-1,2,3,-4,-5]\n",
    "n=len(arr)\n",
    "print(maxcontsum(arr,n))"
   ]
